<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.triagetwo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.triage.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.triage.UpdatePatientVitalActivity"
            android:label="@string/title_activity_update_patient_vital" >
        </activity>
        <activity
            android:name="com.example.triage.RegisterPatientActivity"
            android:label="@string/title_activity_register_patient" >
        </activity>
        <activity
            android:name="com.example.triage.PatientStatusActivity"
            android:label="@string/title_activity_patient_status" >
        </activity>
        <activity
            android:name="com.example.triage.DisplayActivity"
            android:label="@string/title_activity_display" >
        </activity>
        <activity
            android:name="com.example.triage.DoctorActivity"
            android:label="@string/title_activity_doctor" >
        </activity>
        <activity
            android:name="com.example.triage.PatientHistoryActivity"
            android:label="@string/title_activity_patient_history" >
        </activity>
        <activity
            android:name="com.example.triage.PatientListActivity"
            android:label="@string/title_activity_patient_list" >
        </activity>
    </application>

</manifest>
